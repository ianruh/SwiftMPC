# file options

--exclude Tests/XCTestManifests.swift

# format options

--allman false
--binarygrouping 4,8
--commas always
--comments indent
--decimalgrouping 3,6
--elseposition same-line
--empty void
--emptybraces spaced
--exponentcase lowercase
--exponentgrouping disabled
--fractiongrouping disabled
--header "// Created 2020 github @ianruh"
--guardelse same-line
--hexgrouping 4,8
--hexliteralcase uppercase
--ifdef no-indent
--indent 4
--indentcase false
--importgrouping testable-bottom
--linebreaks lf
--maxwidth 100
--octalgrouping 4,8
--operatorfunc spaced
--patternlet hoist
--ranges spaced
--self insert
--semicolons never
--stripunusedargs always
--swiftversion 4.2
--trimwhitespace always
--wraparguments preserve
--wrapcollections preserve
